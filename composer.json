{
	"name": "aik099/phpunit-mink",
	"description": "Library for using Mink in PHPUnit tests. Supports test case-wide session sharing.",
	"keywords": ["PHPUnit", "Selenium", "Mink", "Tests"],
	"homepage": "http://github.com/aik099/phpunit-mink",
	"license": "BSD-3-Clause",

	"authors": [
		{
			"name":  "Alexander Obuhovich",
			"email": "aik.bold@gmail.com"
		}
	],

	"require": {
		"php": ">=5.3.2",
		"behat/mink": "1.5",
		"behat/mink-selenium2-driver": "1.1.0",
		"phpunit/phpunit": "3.7.*"
	},

	"require-dev": {
		"mockery/mockery": "dev-protected-method-mocking"
	},

	"autoload": {
		"psr-0": {
			"aik099\\": "./library/"
		}
	},

	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/aik099/mockery"
		}
	]
}
